{
  "name": "KanvasCamera",
  "version": "0.1.0",
  "summary": "A custom camera built for iOS.",
  "homepage": "https://github.tumblr.net/TumblrMobile/orangina",
  "license": {
    "type": "MPLv2",
    "file": "LICENSE"
  },
  "source": {
    "git": "https://github.com/Automattic/Kanvas-iOS.git",
    "branch": "main"
  },
  "authors": {
    "Tony Cheng": "tony@getkanvas.com"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": [
    "4.2"
  ],
  "requires_arc": true,
  "frameworks": [
    "Foundation",
    "GLKit",
    "OpenGLES",
    "UIKit"
  ],
  "source_files": "Classes/**/*.{h,m,swift,vsh,glsl}",
  "resource_bundles": {
    "KanvasCamera": [
      "Resources/*"
    ]
  },
  "user_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "GLES_SILENCE_DEPRECATION=1"
  },
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "GLES_SILENCE_DEPRECATION=1"
  },
  "dependencies": {
  },
  "swift_version": "4.2"
}
